+libdwarf.dylib: dwarf_names.h dwarf_names.c $(OBJS) dwarf_names.o
+	$(CC) $(CFLAGS) $(LDFLAGS) -shared $(OBJS) -Wl,-install_name,$(SONAME)  dwarf_names.o -lelf $(dwfzlib) -o $@
 
 none:
 	echo "do nothing " $@
@@ -222,7 +221,9 @@
 	rm -f *~
 
